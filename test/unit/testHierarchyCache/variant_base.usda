#usda 1.0
(
    defaultPrim = "myVariant"
)

def Xform "myVariant" ()
{
    over "SphereGroup1" ()
    {
        prepend rel reftest = </myVariant/SphereGroup1/sphere>
        over "sphere" ()
        {
            custom string name = "base"
            double test = 1.0
            color3f[] primvars:displayColor = [(1, 0, 0)]
        }
        over "emptyPrim" ()
        {
            def Xform "something1" {
                double somethingNum = 1.0
            }
        }


    }

    def Xform "SphereGroup2"()
    {

    }
}
