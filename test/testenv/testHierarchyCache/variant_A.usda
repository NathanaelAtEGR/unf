#usda 1.0
(
    defaultPrim = "myVariant"
)

def Xform "myVariant" ()
{
    over "SphereGroup1"()
    {
        prepend rel reftest = </myVariant/SphereGroup1/sphere.name>
        over "sphere" ()
        {
            custom string name = "A"
            double test = 2.0
            color3f[] primvars:displayColor = [(1, 1, 1)]
        }
        over "emptyPrim" ()
        {
            def Xform "something2" {
                double somethingNum = 1.0
            }
        }
    }
}
